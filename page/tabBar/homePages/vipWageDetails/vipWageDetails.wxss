
.headers{
  padding: 20rpx 0;
  border-top: 1rpx solid #e0dfe4;
}
.list{
  padding: 10rpx 0; 
  text-indent: 50rpx;
}
.list_title{
  color: #666666;
}
.list_data{
  margin-left: 20rpx;
}
/* 班组明细 */
.vip_title{
  padding: 30rpx 0;
  text-indent: 30rpx;
  font-size: 32rpx;
}

/* 表格 */
.com_tab_title{
  display: flex;
  border-top: 1rpx solid #efeff4;
  background: #fff;
}
.tab_title{
  background: #f1fcff;
}
checkbox{

  transform:scale(.6);
  margin-left: 4rpx;
}
.com_tab_title view{
  border-right: 1px solid #efeff4;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  text-align: center;
}
.com_tab_title .first_checkbox{
  width: 8%;
}
.com_tab_title .last_child{
  width: 12%;
}
.com_tab_title :nth-child(2){
  width: 14%;
}
.com_tab_title :nth-child(3){
  width: 16.5%;
}
.com_tab_title :nth-child(4){
  width: 16.5%;
}
.com_tab_title :nth-child(5){
  width: 16.5%;
}
.com_tab_title :nth-child(6){
  width: 16.5%;
}
/* 确定 */
.all_yes{
  padding: 20rpx 0;
  text-align: center;
  border-radius:10rpx; 
  font-size: 32rpx;
  margin-top: 50rpx;
}
.yes{
 background: #e0dfe4;
 border: 1rpx solid #bbbbbb;
}

