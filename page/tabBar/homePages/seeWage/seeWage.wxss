.header{
  padding-top:20rpx;
}
.search_box{
  background: #f5f5f5;
  text-align: center;
  padding: 10rpx 0;
  font-size: 28rpx;
  color: #b2b2b2;
  border-radius: 8rpx;
  justify-content: center;
}
.search_box icon{
  margin-right: 5rpx;
}

/* content */
.content{
  margin-top: 24rpx;
  padding: 20rpx 0 20rpx 0;
  position: relative;
}
.list{
  padding: 10rpx 0; 
}
.list_title{
  color: #666666;
}
.list_data{
  margin-left: 20rpx;
}
.see_list{
  position: absolute;
  padding: 10rpx 20rpx;
  border: 1rpx solid #2f8ce0;
  color: #2f8ce0;
  border-radius: 8rpx;
  right: 5%;
  bottom: 10%;
}

